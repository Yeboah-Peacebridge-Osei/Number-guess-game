import random

num = random.randint(2, 5)
guess = None

while guess != num:
    guess = input("guess a number between 2 and 5: ")
    guess = int(guess)

    if guess == num:
        print("Well, you did it..but don't be happy..it was just a guess!")
        break
    else:
        print("Don't give up yet bro. Keep going")
